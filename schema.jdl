entity Tablet {
    name String
    androidId String
    macId String
    model String
}
entity TabletUser {
    firstName String
    lastName String
}
entity UserActivity {
    listName String
    total Long
    completed Long
    uniqueName String
}
relationship OneToMany {
	TabletUser to UserActivity{activity}
   	Tablet to TabletUser{tablet}
}


// Use Data Transfer Objects (DTO)
dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

