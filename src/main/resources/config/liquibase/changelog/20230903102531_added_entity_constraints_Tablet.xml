<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
<!--
        Added the constraints for entity Tablet.
    -->
    <changeSet id="20230903102531-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="center_id"
                                 baseTableName="tablet"
                                 constraintName="fk_tablet__center_id"
                                 referencedColumnNames="id"
                                 referencedTableName="center"/>

        <addForeignKeyConstraint baseColumnNames="donor_id"
                                 baseTableName="tablet"
                                 constraintName="fk_tablet__donor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="donor"/>

        <addForeignKeyConstraint baseColumnNames="archived_by_id"
                                 baseTableName="tablet"
                                 constraintName="fk_tablet__archived_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="modified_by_id"
                                 baseTableName="tablet"
                                 constraintName="fk_tablet__modified_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
    </changeSet>
</databaseChangeLog>
