        <addForeignKeyConstraint baseColumnNames="version_id"
                                 baseTableName="file_belongings"
                                 constraintName="fk_file_belongings__version_id"
                                 referencedColumnNames="id"
                                 referencedTableName="version"/>
    </changeSet>
</databaseChangeLog>
