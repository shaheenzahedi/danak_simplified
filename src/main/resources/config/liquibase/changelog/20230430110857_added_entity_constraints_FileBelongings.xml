<!--
        Added the constraints for entity FileBelongings.
    -->
    <changeSet id="20230430110857-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="file_id"
                                 baseTableName="file_belongings"
                                 constraintName="fk_file_belongings__file_id"
                                 referencedColumnNames="id"
